<?xml version='1.0' encoding='utf-8'?>
<widget id="in.moonfood.android" version="1.0.17" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MOONFOOD</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="moonfoodgc1@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon density="mdpi" height="57" src="icon.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreen" value="none" />
</widget>
